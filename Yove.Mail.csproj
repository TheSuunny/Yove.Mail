<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  <PropertyGroup>
    <id>Yove.Mail</id>
    <Version>1.0.7</Version>
    <Authors>Sunny</Authors>
    <NeutralLanguage>en-US, ru-RU</NeutralLanguage>
    <Description>Email Client for Temp-Mail.org</Description>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <PackageTags>temp-mail,email,mail,client</PackageTags>
    <PackageProjectUrl>https://github.com/TheSuunny/Yove.Mail</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/TheSuunny/Yove.Mail</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Yove.Http" Version="1.1.5"/>
    <PackageReference Include="MimeKitLite" Version="2.1.4"/>
  </ItemGroup>
</Project>